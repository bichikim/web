{
  "name": "@winter-love/functions",
  "private": true,
  "version": "1.0.1-alpha.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "concurrently \"npm run build -w\" \"firebase emulators:start --only functions\"",
    "build": "tsc --assumeChangesOnlyAffectDirectDependencies",
    "lint": "eslint --ext .ts \"src/**/*.{ts,tsx}\"",
    "deploy": "firebase deploy --only functions",
    "init": "npm install",
    "logs": "firebase functions:log",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "mocha"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.12.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "firebase-functions-test": "^0.2.3",
    "mocha": "^8.2.1",
    "node-mocks-http": "^1.9.0",
    "typescript": "^4.1.2",
    "ts-node": "^9.1.0"
  },
  "engines": {
    "node": "12"
  }
}
