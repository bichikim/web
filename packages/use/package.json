{
  "name": "@winter-love/use",
  "version": "1.0.34",
  "description": "Vue utils",
  "keywords": [
    "typescript",
    "composition-api",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "ref",
    "hook",
    "composable"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/shared": "^3.3.0",
    "@winter-love/lodash": "^1.0.34",
    "@winter-love/utils": "^1.0.34",
    "immer": "^10.0.2",
    "vue": "^3"
  },
  "devDependencies": {
    "@types/sinon": "10.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d2b83a5ae925fd122a6f46c99d743ede11b86c2f",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  },
  "web-types": [
    "../../web-types.json"
  ]
}
