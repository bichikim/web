{
  "name": "@winter-love/use",
  "version": "1.0.23",
  "description": "Vue utils",
  "keywords": [
    "typescript",
    "composition-api",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "ref",
    "hook",
    "composable"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@winter-love/utils": "^1.0.23",
    "debounce": "^1.2.1",
    "fast-json-stable-stringify": "^2.1.0",
    "lodash": "^4.17.21",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@types/debounce": "1.2.1",
    "@types/sinon": "10.0.11",
    "@vue/test-utils": "2.0.0-rc.17",
    "vue": "latest"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
