{
  "name": "@winter-love/vite-plugin-monorepo-alias",
  "version": "1.0.34",
  "description": "vite monorepo alias plugin",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*",
    "./plugin": {
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    }
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "*.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch --minify false"
  },
  "dependencies": {
    "@types/node": "^20.2.1",
    "@vue/server-renderer": "^3.2.45",
    "node-html-parser": "^6.1.4",
    "vite": "^5",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@winter-love/vite-lib-config": "1.0.29"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d2b83a5ae925fd122a6f46c99d743ede11b86c2f",
  "stage": "alpha",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/plugin.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
