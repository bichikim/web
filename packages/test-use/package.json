{
  "name": "@winter-love/test-use",
  "version": "1.0.23-alpha.25",
  "description": "test composition api utils",
  "keywords": [
    "typescript",
    "utils",
    "functional"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "dev": "tsup-node --watch --legacy-output",
    "prepare": "tsup-node --dts --legacy-output"
  },
  "dependencies": {
    "@vue/test-utils": "next",
    "@winter-love/utils": "^1.0.23-alpha.25",
    "vue": "^3.2.30"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "lodash": "^4.17.21"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
