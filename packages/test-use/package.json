{
  "name": "@winter-love/test-use",
  "version": "1.0.25-alpha.14",
  "description": "test composition api utils",
  "keywords": [
    "typescript",
    "utils",
    "functional"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@vue/test-utils": "^2.0.0",
    "@winter-love/utils": "workspace:*",
    "vue-demi": "*"
  },
  "devDependencies": {
    "lodash": "4.17.21",
    "vue": "^3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": {
      "resolve": false
    },
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
