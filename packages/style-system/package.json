{
  "name": "@winter-love/style-system",
  "version": "1.0.23-alpha.14",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "dev": "tsup-node --watch --legacy-output",
    "prepare": "tsup-node --dts --legacy-output"
  },
  "dependencies": {
    "@winter-love/utils": "^1.0.23-alpha.14",
    "merge-deep": "^3.0.3"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@types/merge-deep": "^3.0.0",
    "@types/styled-system": "^5.1.11",
    "@types/styled-system__css": "^5.0.15",
    "@vue/test-utils": "^2.0.0-rc.14",
    "@winter-love/emotion": "^1.0.23-alpha.14",
    "csstype": "^3.0.8",
    "gulp": "^4.0.2",
    "vue": "next",
    "vue-demi": "latest"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
