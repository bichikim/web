{
  "name": "@winter-love/style-system",
  "version": "1.0.24-alpha.1",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@winter-love/utils": "^1.0.24-alpha.1",
    "merge-deep": "^3.0.3"
  },
  "devDependencies": {
    "@emotion/css": "11.7.1",
    "@types/merge-deep": "3.0.0",
    "@types/styled-system": "5.1.15",
    "@types/styled-system__css": "5.0.16",
    "@winter-love/emotion": "^1.0.24-alpha.1",
    "csstype": "3.0.11",
    "vue": "latest",
    "vue-demi": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
