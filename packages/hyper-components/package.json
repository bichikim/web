{
  "name": "@winter-love/hyper-components",
  "version": "1.0.23-alpha.15",
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "dev": "tsup-node --watch --legacy-output",
    "prepare": "tsup-node --dts --legacy-output"
  },
  "dependencies": {
    "@winter-love/stitches": "^1.0.23-alpha.15",
    "@winter-love/utils": "^1.0.23-alpha.15",
    "color": "^4.1.0",
    "quasar": "^2.4.2",
    "vue-demi": "latest",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "csstype": "^3.0.8",
    "gulp": "^4.0.2",
    "vue": "next"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
