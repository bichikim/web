{
  "name": "@winter-love/hyper-components",
  "version": "1.0.24-alpha.0",
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "quasar": "^2.6.0",
    "@stitches/core": "1.2.7",
    "@winter-love/stitches": "^1.0.24-alpha.0",
    "@winter-love/utils": "^1.0.23",
    "clsx": "^1.1.1",
    "color": "^4.2.0",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "csstype": "3.0.11",
    "@vue/server-renderer": "^3.2.31",
    "vue": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
