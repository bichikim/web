{
  "name": "@winter-love/hyper-components",
  "version": "1.0.25-alpha.14",
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "fix": "vue-demi-fix",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@stitches/core": "^1.2.8",
    "@winter-love/lodash": "^1.0.25-alpha.14",
    "@winter-love/stitches": "workspace:*",
    "@winter-love/utils": "^1.0.25-alpha.14",
    "clsx": "^1.1.1",
    "color": "^4.2.0",
    "quasar": "^2.6",
    "vue-demi": "^0.13.1"
  },
  "devDependencies": {
    "@vue/server-renderer": "^3.2",
    "csstype": "3.0.11",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vue": "^3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": {
      "resolve": false
    },
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
