{
  "name": "@winter-love/utils",
  "version": "1.0.35",
  "description": "Useful javascript utils",
  "keywords": [
    "typescript",
    "utils",
    "functional"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch --minify false",
    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@winter-love/lodash": "workspace:*",
    "safe-stable-stringify": "^2.5.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "smooth-scroll-into-view-if-needed": "^2.0.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@winter-love/vite-lib-config": "workspace:*",
    "js-cookie": "^3.0.5",
    "micro-memoize": "^4.1.2",
    "tsd": "^0.31.2",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d2b83a5ae925fd122a6f46c99d743ede11b86c2f",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
