{
  "name": "@winter-love/utils",
  "version": "1.0.25-alpha.14",
  "description": "Useful javascript utils",
  "keywords": [
    "typescript",
    "utils",
    "functional"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@winter-love/lodash": "^1.0.25-alpha.14",
    "fast-json-stable-stringify": "^2.1.0",
    "react-fast-compare": "^3.2.0"
  },
  "devDependencies": {
    "@types/debounce": "1.2.1",
    "@types/js-cookie": "3.0.1",
    "js-cookie": "^3.0.1",
    "micro-memoize": "^4.0.9",
    "tsd": "0.20.0",
    "vue-demi": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
