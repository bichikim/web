{
  "name": "@winter-love/utils",
  "version": "1.0.23-alpha.18",
  "description": "Useful javascript utils",
  "keywords": [
    "typescript",
    "utils",
    "functional"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "dev": "tsup-node --watch --legacy-output",
    "prepare": "tsup-node --dts --legacy-output"
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "micro-memoize": "^4.0.9",
    "react-fast-compare": "^3.2.0"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "vue-demi": "latest"
  },
  "gitHead": "5aa1e8493f474f75436be59c2e9b0df86550235e",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
