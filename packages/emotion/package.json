{
  "name": "@winter-love/emotion",
  "version": "1.0.23-alpha.11",
  "description": "Emotion for Vue",
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "emotion",
    "css-in-js"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "prepare": "tsup-node --dts --legacy-output",
    "dev": "tsup-node --watch --legacy-output"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/serialize": "^1.0.2",
    "@emotion/utils": "^1.0.0",
    "@winter-love/use": "^1.0.23-alpha.11",
    "@winter-love/utils": "^1.0.23-alpha.5",
    "clsx": "^1.1.1",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@emotion/server": "^11.4.0",
    "@vue/server-renderer": "latest",
    "@vue/test-utils": "next",
    "gulp": "^4.0.2",
    "vue": "next"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "clean": true,
    "format": ["cjs", "esm", "iife"],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58"
}
