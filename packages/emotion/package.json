{
  "name": "@winter-love/emotion",
  "version": "1.0.25-alpha.0",
  "description": "Emotion for Vue",
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "emotion",
    "css-in-js"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/serialize": "^1.0.2",
    "@emotion/utils": "^1.1.0",
    "@winter-love/use": "^1.0.25-alpha.0",
    "@winter-love/utils": "^1.0.25-alpha.0",
    "clsx": "^1.1.1",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@emotion/server": "11.4.0",
    "@vue/server-renderer": "latest",
    "vue": "3.2.31"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
