{
  "name": "@winter-love/emotion",
  "version": "1.0.25-alpha.18",
  "description": "Emotion for Vue",
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "emotion",
    "css-in-js"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/serialize": "^1.1.0",
    "@emotion/utils": "^1.2.0",
    "@winter-love/use": "workspace:1.0.25-alpha.18",
    "@winter-love/utils": "workspace:1.0.25-alpha.18",
    "clsx": "^1.2.1",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@emotion/server": "^11.10.0",
    "@vue/server-renderer": "^3.2.41"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "0f9585327d3bbfd468f48073be6977aeb17c9e78",
  "stage": "deprecated",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
