{
  "name": "@winter-love/emotion",
  "version": "1.0.23",
  "description": "Emotion for Vue",
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "emotion",
    "css-in-js"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/serialize": "^1.0.2",
    "@emotion/utils": "^1.0.0",
    "@winter-love/use": "^1.0.23",
    "@winter-love/utils": "^1.0.23",
    "clsx": "^1.1.1",
    "vue-demi": "^0.12.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@emotion/server": "^11.4.0",
    "@vue/server-renderer": "latest",
    "gulp": "^4.0.2",
    "vue": "latest"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
