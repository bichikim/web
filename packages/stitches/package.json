{
  "name": "@winter-love/stitches",
  "version": "1.0.25-alpha.15",
  "repository": "git@github.com:winter-love/web.git",
  "author": "uni@meta-z.io",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts",
    "publish": "npm publish"
  },
  "dependencies": {
    "@stitches/core": "^1.2.8",
    "@vue/runtime-core": "^3.2.39",
    "@winter-love/lodash": "workspace:*",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@vue/server-renderer": "^3.2.39",
    "lodash": "^4.17.21",
    "tsd": "^0.22.0",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "gitHead": "8e317dee75aded4e8a6d9d1f2556a5a7ab3bdb93",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
