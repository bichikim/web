{
  "name": "@winter-love/stitches",
  "version": "1.0.25-alpha.14",
  "description": "Stitches for Vue",
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "stitches",
    "css-in-js"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "fix": "vue-demi-fix",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@stitches/core": "^1.2.8",
    "@winter-love/utils": "workspace:*",
    "vue-demi": "*"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@vue/server-renderer": "^3.2",
    "typescript": "^4.7.4",
    "vue": "^3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
