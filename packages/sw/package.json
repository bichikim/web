  },
  "devDependencies": {
    "@types/serviceworker": "^0.0.111",
    "vitest": "*",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "@winter-love/vite-lib-config": "workspace:*"
  }
