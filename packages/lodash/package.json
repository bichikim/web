{
  "name": "@winter-love/lodash",
  "version": "1.0.27-alpha.4",
  "description": "Stitches for Vue",
  "keywords": [
    "lodash"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "generate-lodash-es": "lodash modularize exports=es -d -s -o src/lodash",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@types/lodash-es": "4.17.2"
  },
  "devDependencies": {
    "lodash-cli": "4.17.5",
    "tsup": "^6.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b8f8c59a3c3b040b94cd578439949acde8212f5d",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
