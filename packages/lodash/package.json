{
  "name": "@winter-love/lodash",
  "version": "1.0.25-alpha.14",
  "description": "Stitches for Vue",
  "keywords": [
    "lodash"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "generate-lodash-es": "lodash modularize exports=es -d -s -o src/lodash",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.2"
  },
  "devDependencies": {
    "lodash-cli": "4.17.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
