{
  "name": "vare",
  "version": "1.0.24-alpha.0",
  "private": false,
  "description": "share state for vue components",
  "keywords": [
    "typescript",
    "vuex",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "redux",
    "reactive",
    "mobx"
  ],
  "homepage": "https://github.com/winter-love/web/tree/dev/packages/vare",
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "test-dev": "vite",
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.13",
    "@winter-love/utils": "^1.0.23",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "vue-router": "^4.0.14",
    "@vitejs/plugin-vue": "2.2.4",
    "@vitejs/plugin-vue-jsx": "1.3.8",
    "@winter-love/use": "^1.0.23",
    "@zerollup/ts-transform-paths": "1.7.18",
    "ttypescript": "1.5.13",
    "vite": "2.8.6",
    "vue": "3.2.31"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
