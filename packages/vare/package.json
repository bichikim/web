{
  "name": "vare",
  "version": "1.0.23-alpha.11",
  "description": "share state for vue components",
  "private": false,
  "keywords": [
    "typescript",
    "vuex",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "redux",
    "reactive",
    "mobx"
  ],
  "homepage": "https://github.com/winter-love/web/tree/dev/packages/vare",
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup --dts --legacy-output --minify",
    "prepare": "tsup --dts --legacy-output",
    "dev": "tsup --watch --legacy-output"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.0-beta.19",
    "@winter-love/utils": "^1.0.23-alpha.5",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@winter-love/use": "^1.0.23-alpha.11",
    "gulp": "^4.0.2",
    "vue": "next"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "clean": true,
    "format": ["cjs", "esm", "iife"],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58"
}
