{
  "name": "vare",
  "version": "1.0.25-alpha.15",
  "description": "share state for vue components",
  "keywords": [
    "typescript",
    "vuex",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "composition",
    "redux",
    "reactive",
    "mobx",
    "vare",
    "type",
    "safe"
  ],
  "homepage": "https://github.com/winter-love/web/tree/dev/packages/vare",
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./simple-store": {
      "import": "./dist/simple-store/index.mjs",
      "require": "./dist/simple-store/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "fix": "npx vue-demi-fix",
    "prepare": "tsup --dts",
    "test-dev": "vite"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.1.3",
    "@winter-love/utils": "^1.0.25-alpha.15",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "vite": "^2.9.1",
    "vue-router": "^4.0.14"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "8e317dee75aded4e8a6d9d1f2556a5a7ab3bdb93",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/simple-store/index.ts"
    ],
    "dts": {
      "resolve": false
    },
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
