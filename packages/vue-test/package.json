{
  "name": "@winter-love/vue-test",
  "version": "1.0.27-alpha.1",
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts",
    "publish": "npm publish"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.41",
    "@vue/test-utils": "^2.2.0",
    "jest-mock": "^29.2.1",
    "node-html-parser": "^5.4.2",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.4",
    "lodash": "^4.17.21",
    "tsd": "^0.22.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "86d2019adab7f93e8188544a2a4f8fbcb999043a",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
