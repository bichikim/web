{
  "name": "@winter-love/vue-test",
  "version": "1.0.27",
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.45",
    "@vue/test-utils": "^2.2.6",
    "jest-mock": "^29.3.1",
    "node-html-parser": "^6.1.4",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.14",
    "lodash": "^4.17.21",
    "tsd": "^0.24.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "4a8bc8b824e5780a82d3b2958e1a8afb48a49bb0",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
