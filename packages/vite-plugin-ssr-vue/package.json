{
  "name": "vite-plugin-ssr-vue",
  "version": "1.0.23-alpha.25",
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/iife/index.js",
  "unpkg": "lib/iife/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "tsup-node --dts --legacy-output --minify",
    "dev": "tsup-node --watch --legacy-output",
    "prepare": "tsup-node --dts --legacy-output"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.31",
    "node-html-parser": "^5.2.0",
    "vite": "^2.8.4",
    "vue": "latest"
  },
  "devDependencies": {
    "gulp": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "66c79bd0d5c23a437b8170f758ff58e4496afb58",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
