{
  "name": "vite-plugin-ssr-vue",
  "version": "1.0.24",
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*",
    "./plugin": {
      "import": "./lib/plugin.mjs",
      "require": "./lib/plugin.js"
    }
  },
  "main": "lib/index.js",
  "jsdelivr": "lib/index.global.js",
  "unpkg": "lib/index.global.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup-node --dts --minify",
    "dev": "tsup-node --watch",
    "prepare": "tsup-node --dts"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.31",
    "node-html-parser": "^5.2.0",
    "vite": "^2.8.4",
    "vue": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a00563d4b5288df4b4bbe713ce9b970e1844ca29",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/plugin.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "lib",
    "sourcemap": true,
    "splitting": false
  }
}
