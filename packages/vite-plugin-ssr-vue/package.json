{
  "name": "vite-plugin-ssr-vue",
  "version": "1.0.27-alpha.5",
  "private": true,
  "description": "hyper components base on quasar and stitches",
  "keywords": [
    "typescript",
    "style",
    "style-system",
    "component",
    "ui",
    "css",
    "store",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*",
    "./plugin": {
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    }
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.41",
    "node-html-parser": "^6.0.0",
    "vue": "^3.2.45"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "cace853a74de291e423dc79447ce0db596fdf60d",
  "stage": "alpha",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/plugin.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
