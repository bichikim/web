{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "jsx": "preserve",
    "types": ["@types/jest"],
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "stores/*": [
        "src/stores/*"
      ]
    }
  },
  "exclude": ["/dist", ".quasar", "node_modules"]
}
