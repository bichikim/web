{
  "name": "@coong/client",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 8080 --open",
    "dev:ssr": "ts-node server/index.ts",
    "build": "yarn run build:ssr",
    "build:spa": "cross-env NODE_ENV=production vite build --outDir dist/spa",
    "build:ssr": "yarn run build:client && yarn run build:server && yarn run build:server-run",
    "build:client": "cross-env NODE_ENV=production vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env NODE_ENV=production vite build --ssr src/create-app.ts --outDir dist/server",
    "build:server-run": "tsc --project tsconfig.server.json",
    "start": "yarn run start:ssr",
    "start:spa": "vite preview --host --https",
    "start:ssr": "cross-env NODE_ENV=production node dist/server-run/index.js"
  },
  "dependencies": {
    "@capacitor/cli": "^3.2.0",
    "@capacitor/core": "^3.2.0",
    "@emotion/css": "^11.1.3",
    "@emotion/server": "^11.4.0",
    "@quasar/app": "^3.0.4",
    "@vue/server-renderer": "^3.2.6",
    "@winter-love/emotion": "^1.0.17-alpha.7",
    "@winter-love/style-system-next": "^1.0.17-alpha.7",
    "@winter-love/utils": "^1.0.17-alpha.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "node-inject-html": "^0.0.5",
    "quasar": "^2.0.4",
    "serve-static": "^1.14.1",
    "vue": "3.2.6",
    "vue-i18n": "9.1.7",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.395",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.6.2",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "csstype": "^3.0.8",
    "rollup-plugin-analyzer": "^4.0.0",
    "ts-node": "^10.2.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "vite": "^2.5.1",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-icons": "^0.6.5",
    "vite-plugin-pwa": "^0.11.0"
  }
}
