{
  "name": "@coong/client",
  "version": "1.0.0",
  "scripts": {
    "sync:ios": "cap sync ios",
    "sync:android": "cap sync android",
    "dev:ios": "cap run ios -l --exrernal",
    "dev:android": "cap run android -l --exrernal",
    "dev:spa": "vite",
    "dev:ssr": "cross-env MODE=ssr vite",
    "dev:run-ssr": "ts-node run-ssr/index.ts",
