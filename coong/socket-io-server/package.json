{
  "name": "@coong/socket-io-server",
  "version": "1.0.27-alpha.10",
  "private": true,
  "description": "Vue utils",
  "keywords": [
    "typescript",
    "composition-api",
    "vue",
    "vue3",
    "vuejs",
    "vuejs3",
    "ref",
    "hook",
    "composable"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@winter-love/lodash": "workspace:^1.0.27-alpha.5",
    "@winter-love/utils": "workspace:^1.0.27-alpha.10",
    "socket.io": "^4.5.3"
  },
  "devDependencies": {
    "@types/debounce": "1.2.1",
    "@types/node": "18.11.9",
    "@types/sinon": "10.0.13",
    "@vue/server-renderer": "3.2.45",
    "@vue/test-utils": "2.2.3",
    "debounce": "^1.2.1",
    "vue": "3.2.45"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "162bc0badb2b0932df3c9dc51cc1ff7af3587140",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
