    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-babel": "1.1.2",
