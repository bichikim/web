    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.10",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-babel": "1.1.2",
    "vite-plugin-babel-compiler": "^0.3.0",
    "vite-plugin-imp": "^2.3.0",
    "vite-plugin-pwa": "^0.13.1",
    "vite-tsconfig-paths": "^3.5.1"
