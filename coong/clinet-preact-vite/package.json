    "express": "^4.18.2",
    "graphql": "16.3.0",
    "graphql-tag": "^2.12.6",
    "preact": "^10.11.2",
    "preact-router": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
