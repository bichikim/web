    "@winter-love/lodash": "^1.0.25-alpha.16",
    "express": "^4.18.1",
    "graphql": "16.3.0",
    "graphql-tag": "^2.12.6",
    "preact": "^10.11.1",
    "preact-router": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/add": "3.2.1",
