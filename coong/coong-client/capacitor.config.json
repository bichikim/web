{
  "appId": "io.coong",
  "appName": "Coong",
  "bundledWebRuntime": false,
  "npmClient": "pnpm",
  "webDir": "dist/spa",
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": true,
      "backgroundColor": "#000000ff",
      "androidScaleType": "CENTER_CROP"
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "Badge": {
      "persist": true,
      "autoClear": false
    }
  },
  "cordova": {
  }
}
