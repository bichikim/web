{
  "name": "@winter-love/client-vite",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build:spa": "vite build --outDir dist/spa",
    "dev:spa": "vite --host",
    "dev:ssr": "MODE_SSR=true ts-node server/index.ts",
