  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.4",
    "@winter-love/lodash": "^1.0.25-alpha.14",
    "express": "^4.18.1",
    "graphql": "16.3.0",
    "vare": "workspace:*",
    "vue": "3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.64",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.15.4",
    "dotenv": "^16.0.1",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.4",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-imp": "^2.2.0",
    "vite-plugin-md": "^0.14.1",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-ssr": "^0.3.64",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "tsup": {
    "clean": true,
    "format": [
      "cjs"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
