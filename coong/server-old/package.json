{
  "name": "@coong/server-old",
  "version": "1.0.25-alpha.8",
  "private": true,
  "scripts": {
    "build": "rimraf ./lib && pnpm run generate && ttsc -p tsconfig.build.json",
    "dev": "cross-env NODE_ENV=development ts-node-dev --clear -r tsconfig-paths/register src/index.ts",
    "generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "generate:watch": "prisma generate --schema ./src/prisma/schema.prisma --watch",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "3.11.1",
    "@winter-love/utils": "^1.0.25-alpha.6",
    "apollo-server": "3.6.4",
    "bcryptjs": "2.4.3",
    "class-validator": "0.13.2",
    "express": "4.17.3",
    "express-graphql": "0.12.0",
    "graphql": "15.8.0",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.17.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "tslib": "2.3.1",
    "type-graphql": "1.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/graphql-fields": "1.3.4",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.21",
    "@zerollup/ts-transform-paths": "1.7.18",
    "cross-env": "7.0.3",
    "prisma": "3.11.1",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.14.1",
    "ttypescript": "1.5.13",
    "typegraphql-prisma": "0.19.2",
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "vite-plugin-node": "0.0.19"
  }
}
