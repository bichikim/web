    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0",
