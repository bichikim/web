{
  "name": "@coong/server",
  "version": "1.0.27-alpha.11",
  "private": true,
  "scripts": {
    "build": "rimraf ./lib && pnpm run prisma:generate && ttsc -p tsconfig.build.json",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --clear -r tsconfig-paths/register src/index.ts",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:generate:watch": "prisma generate --schema ./src/prisma/schema.prisma --watch",
    "prisma:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "~4.6.0",
    "@winter-love/utils": "workspace:^1.0.27-alpha.11",
    "apollo-server": "3.6.4",
    "apollo-server-core": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "apollo-server-plugin-base": "^3.6.3",
    "bcryptjs": "2.4.3",
    "class-validator": "0.13.2",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "graphql": "15.8.0",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.20.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.14",
    "@types/graphql-fields": "1.3.4",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.9",
    "@zerollup/ts-transform-paths": "1.7.18",
    "cross-env": "7.0.3",
    "graphql-scalars": "^1.19.0",
    "prisma": "~4.6.0",
    "rimraf": "3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "ttypescript": "1.5.13",
    "typegraphql-prisma": "0.23.0",
    "typescript": "4.9.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": true
  }
}
