{
  "name": "coong",
  "private": true,
  "description": "coong web & app service",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "dev:spa": "pnpm run dev --filter @winter-love/web... --parallel",
    "dev:libray": "pnpm run dev --filter libraries... --parallel",
    "build:spa": "pnpm run build --filter @winter-love/web... --stream",
    "build:libray": "pnpm run build --filter libraries... --stream",
    "version:patch": "lerna version patch",
    "publish:patch": "lerna publish from-package",
    "format:package.json": "sort-package-json \"package.json\" \"packages/*/package.json\" ",
    "version:next": "lerna version prerelease --preid next",
    "publish:next": "lerna publish from-package --preid next",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
    "storybook": "start-storybook -p 6006",
    "clean": "lerna clean --yes && rimraf node_modules",
    "test": "jest"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
