    "sinon": "^11.1.2",
    "sort-package-json": "^1.52.0",
    "ts-node": "^10.2.1",
    "tsd": "^0.17.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "vitepress": "^0.15.6",
    "vue": "^3.2.19",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "engines": {
    "node": ">= 10.18.1",
    "yarn": ">= 3"
  },
  "productName": "Coong",
  "packageManager": "yarn@3.0.0"
}
