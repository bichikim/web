{
  "name": "coong",
  "version": "0.0.1",
  "private": true,
  "description": "coong web & app service",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "workspaces": {
    "packages": [
      "packages/*",
      "server"
    ]
  },
  "scripts": {
    "build": "quasar build",
    "build:server": "yarn workspaces foreach -tR --from coong-server run build ",
    "build:spa": "quasar build -m spa",
    "build:libraries": "yarn workspaces foreach -tR --from libraries run build ",
    "build:with-libraries": "yarn workspaces foreach -tR run build",
    "dev": "quasar dev",
    "dev:spa": "quasar dev -m spa",
    "dev:with-libraries": "yarn workspaces foreach -ptR run dev",
    "dev:android": "quasar dev -m capacitor -T android",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "version:patch": "lerna version patch",
    "version:patch-force": "lerna version patch --force-publish",
    "publish:patch": "lerna publish from-package",
    "format:package.json": "sort-package-json \"package.json\" \"packages/*/package.json\" \"server/package.json\" ",
    "version:next": "lerna version prerelease --preid next",
    "publish:next": "lerna publish from-package --preid next --dist-tag next",
    "version:beta": "lerna varsion prerelease --preid beta",
    "publish:beta": "lerna publish from-package --preid beta --dist-tag beta",
    "version:alpha": "lerna version prerelease --preid alpha",
    "publish:alpha": "lerna publish from-package --preid alpha --dist-tag alpha",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
    "storybook": "start-storybook -p 6006",
    "clean": "lerna clean --yes && rimraf node_modules",
    "start:local-docker": "docker-compose up -d",
    "test": "jest",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
