{
  "name": "web",
  "private": true,
  "repository": "git@github.com:winter-love/web.git",
  "author": "bichi@live.co.kr",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build:web": "pnpm run build --filter @winter-love/web... --stream",
    "build:libraries": "pnpm install && pnpm run build --filter libraries... --stream",
    "clean": "rimraf node_modules && lerna clean -y",
    "dev:server": "pnpm run dev --filter @winter-love/server... --parallel",
    "dev:spa": "pnpm run dev --filter @winter-love/web... --parallel",
    "dev:pwa": "pnpm run dev:pwa --filter @winter-love/web... --parallel",
    "dev:ssr": "pnpm run dev:ssr --filter @winter-love/web... --parallel",
    "publish": "pnpm publish --tag next --filter @winter-love/*",
    "format:package.json": "sort-package-json \"package.json\" \"packages/*/package.json\" ",
    "lint": "eslint --ext .ts \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "dependencies": {
    "@quasar/extras": "^1.10.2",
    "quasar": "^2.0.0-beta.12"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@quasar/app": "^3.0.0-beta.15",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@types/chai": "^4.2.16",
    "@types/gulp": "^4.0.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/through2": "^2.0.36",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vue/babel-plugin-transform-vue-jsx": "^1.2.1",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "del": "^6.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.9.0",
    "gulp": "^4.0.2",
    "gulp-execa": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "lerna": "^4.0.0",
    "rollup": "^2.45.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-smart-asset": "^2.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts-treeshaking": "^1.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sort-package-json": "^1.49.0",
    "through2": "^4.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2",
    "vinyl-paths": "^3.0.1",
    "vue-jest": "^4.0.1",
    "vue-pwa-asset-generator": "^1.6.0"
  },
  "engines": {
    "node": ">=12"
  }
}
