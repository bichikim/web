{
  "name": "@winter-love/web",
  "version": "0.0.1",
  "private": true,
  "description": "web & app service",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "scripts": {
    "build:android": "ionic cap build android",
    "build:coong-client": "pnpm --filter @winter-love/coong-client run build:spa ",
    "build:docs": "vitepress build docs",
    "clean": "lerna clean --yes && rimraf node_modules",
    "dev:android": "ionic cap run android -l --external --open",
    "dev:docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "format:package.json": "bunx sort-package-json \\\"package.json\\\" \\\"packages/*/package.json\\\" \\\"apps/*/package.json\\\" ",
    "lint": "eslint --ext .js,.ts,.tsx,.vue ./",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx,.vue ./",
    "publish:alpha": "lerna publish from-package --preid alpha --dist-tag alpha",
    "publish:beta": "lerna publish from-package --preid beta --dist-tag beta",
    "publish:next": "lerna publish from-package --preid next --dist-tag next",
    "publish:patch": "lerna publish patch --no-private",
    "start:local-docker": "docker-compose -f local/docker-compose.yml up -d",
    "story:build": "histoire build",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview",
    "sync:android": "ionic cap sync android",
    "test": "jest",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest watch",
    "version:alpha": "lerna version prerelease --preid alpha",
    "version:beta": "lerna version prerelease --preid beta",
    "version:next": "lerna version prerelease --preid next",
    "version:patch": "lerna version patch",
    "version:patch-force": "lerna version patch --force-publish",
    "storybook": "storybook dev -p 6006",
    "test:storybook": "test-storybook"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
