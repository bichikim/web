{
  "name": "web",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev:web": "pnpm recursive run dev --filter @winter-love/web... --filter @winter-love/functions --no-sort --workspace-concurrency 10",
    "dev:web-snow": "pnpm recursive run dev --filter @winter-love/web-snow... --filter @winter-love/functions --no-sort --workspace-concurrency 10",
    "dev:web-vite": "pnpm recursive run dev --filter @winter-love/web-vite... --filter @winter-love/functions --no-sort --workspace-concurrency 10",
    "clean": "rimraf node_modules && lerna clean",
    "build": "pnpm recursive run build",
    "lint": "eslint --ext .ts \"src/**/*.{ts,tsx}\"",
    "functions": "yarn workspace functions"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@types/chai": "^4.2.14",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.0",
    "@types/through2": "^2.0.36",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "del": "^6.0.0",
    "esbuild": "^0.8.33",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.4.1",
    "gulp": "^4.0.2",
    "gulp-execa": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "lerna": "^3.22.1",
    "rollup": "^2.37.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-esbuild": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "through2": "^4.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "vinyl-paths": "^3.0.1",
    "vue-pwa-asset-generator": "^1.6.0",
    "webpack": "^5.15.0"
  }
}
