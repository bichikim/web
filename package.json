    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "@testing-library/vue": "^6.5.0",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.175",
    "@types/merge-deep": "^3.0.0",
