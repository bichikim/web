{
  "name": "coong-server",
  "version": "1.0.17-alpha.0",
  "private": true,
  "scripts": {
    "start": "node ./lib/index.js",
    "build": "rimraf ./lib && yarn run generate && ttsc",
    "dev": "rimraf ./lib && tsc-watch --compiler ttypescript/bin/tsc --onSuccess \"cross-env NODE_ENV=development node ./lib/index.js",
    "generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "generate:watch": "prisma generate --schema ./src/prisma/schema.prisma --watch"
  },
  "dependencies": {
    "@prisma/client": "~2.27.0",
    "@winter-love/utils": "^1.0.17-alpha.0",
    "apollo-server": "^3.0.2",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/ramda": "^0.27.38",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "prisma": "~2.27.0",
    "tsc-watch": "^4.2.9",
    "ttypescript": "^1.5.12",
    "typegraphql-prisma": "^0.14.7",
    "typescript": "^4.3.4"
  }
}
